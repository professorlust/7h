<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher"
        xmlns:awe="http://schemas.awesomium.com/winfx" x:Class="Launcher.MainWindow"
        mc:Ignorable="d"
        Title="OVA Remake Launcher" Height="768" Width="1024" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Pictures/Cloud.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <awe:WebControl Margin="10,10,0,10" HorizontalAlignment="Left" Width="690" Source="http://ovaremake.mooo.com/"/>
        <Button x:Name="button" Content="Launch" HorizontalAlignment="Left" Height="85" Margin="755,625,0,0" VerticalAlignment="Top" Width="220" Click="bLaunch" Background="#7F000000" BorderBrush="Black" FontFamily="Swis721 BlkCn BT" FontWeight="Bold" FontSize="48"/>
    </Grid>
</Window>
